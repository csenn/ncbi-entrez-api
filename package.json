{
  "name": "ncbi-api",
  "version": "1.0.0",
  "description": "Stream compatible wrapper around NCBI Entrez API",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile"
  },
  "author": "Craig Sennabaum",
  "license": "ISC",
  "dependencies": {
    "lodash": "^3.10.1",
    "request": "^2.65.0"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.1.18",
    "jest-cli": "^0.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csenn/ncbi-entrez-api"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
